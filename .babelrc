{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry",
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "Firefox 52",
            "Chrome 48",
            "not ie <= 8"
          ]
        }
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "angularjs-annotate",
    "lodash"
  ]
}
